{
  "source": "./src",
  "destination": "./docs",
  "includes": ["\\.(jsx?)$"],
  "plugins": [
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {
            "from": "^src/js/",
            "to": ""
          },
          {
            "from": "\\.jsx$",
            "to": ".js"
          }
        ]
      }
    }
  ],
  "experimentalProposal": {
    "objectRestSpread": true
  }
}
